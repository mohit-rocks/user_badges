{#
/**
 * @file user_badge.html.twig
 * Default theme implementation to present User badge data.
 *
 * This template is used when viewing User badge pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_user_badge()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('user_badge') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
